{
  "id": "merge-k-sorted-lists",
  "functionName": "mergeKLists",
  "testCases": [
    { "input": "[[[1,4,5],[1,3,4],[2,6]]]", "expected": "[1,1,2,3,4,4,5,6]" },
    { "input": "[[]]", "expected": "[]" },
    { "input": "[[[]]]", "expected": "[]" },
    { "input": "[[[1,2,3],[4,5,6],[7,8,9]]]", "expected": "[1,2,3,4,5,6,7,8,9]" },
    { "input": "[[[1,3,5],[2,4,6],[0,7,8]]]", "expected": "[0,1,2,3,4,5,6,7,8]" }
  ]
} 