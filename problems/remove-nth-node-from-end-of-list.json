{
  "id": "remove-nth-node-from-end-of-list",
  "functionName": "removeNthFromEnd",
  "testCases": [
    { "input": "[[1,2,3,4,5], 2]", "expected": "[1,2,3,5]" },
    { "input": "[[1], 1]", "expected": "[]" },
    { "input": "[[1,2], 1]", "expected": "[1]" },
    { "input": "[[1,2], 2]", "expected": "[2]" },
    { "input": "[[1,2,3], 3]", "expected": "[2,3]" }
  ]
} 