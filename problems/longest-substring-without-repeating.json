{
  "id": "longest-substring-without-repeating",
  "functionName": "lengthOfLongestSubstring",
  "testCases": [
    { "input": "[\"abcabcbb\"]", "expected": "3" },
    { "input": "[\"bbbbb\"]", "expected": "1" },
    { "input": "[\"pwwkew\"]", "expected": "3" },
    { "input": "[\"\"]", "expected": "0" },
    { "input": "[\"au\"]", "expected": "2" },
    { "input": "[\"dvdf\"]", "expected": "3" }
  ]
} 